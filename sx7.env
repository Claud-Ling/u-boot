#!/bin/sh
# uboot build env file for sx7 project

#
# cross compile define
#
CROSS_COMPILE=arm-none-linux-gnueabi-
if [ -z "$(${CROSS_COMPILE}gcc --version 2> /dev/null)" ]; then
	echo "${CROSS_COMPILE}gcc: command not found. be sure it's in your PATH!" >& 2
	unset CROSS_COMPILE
else
	export CROSS_COMPILE
fi
